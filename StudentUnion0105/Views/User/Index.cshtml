@model List<StudentUnion0105.ViewModels.SuObjectVM>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}@{
    ViewBag.Title = "Users";
}@{
    Layout = "_LayoutAdmin";
}

<h1 class="font-weight-bold">
    <img src="~/Images/Icons/User.png" height="60" width="60" /> Users
</h1>
<div class="table-responsive">
    <table class="table table-bordered table-sm table-hover">

        <thead class="thead-dark">
            <tr><th>ID</th><th>Name</th><th>Email</th><th></th><th></th><th></th><th></th><th></th></tr>
        </thead>
        @foreach (var x in Model)

        {
            <tr>
                <td>@x.Description2</td>
                <td>@x.Name</td>
                <td>@x.Description</td>
                <td style="width: 22px"><a asp-controller="User" asp-action="Edit" asp-route-Id="@x.Description2"><img src="~/Images/Icons/open.png" title="Edit" height="20" width="20" /></a></td>
                <td style="width: 22px"><a asp-controller="UserOrganization" asp-action="Index" asp-route-id="@x.Description2"><img src="~/Images/Icons/Organization.png" title="Organization" height="20" width="20" /></a></td>
                <td style="width: 22px"><a asp-controller="UserProject" asp-action="Index" asp-route-id="@x.Description2"><img src="~/Images/Icons/Project.png" title="Project" height="20" width="20" /></a></td>
                <td style="width: 22px"><a asp-controller="UserRelation" asp-action="Index" asp-route-id="@x.Description2"><img src="~/Images/Icons/relation.png" title="Relation" height="20" width="20" /></a></td>
                <td style="width: 22px"><a asp-controller="UserRole" asp-action="Index" asp-route-id="@x.Description2"><img src="~/Images/Icons/Role.png" title="Roles" height="20" width="20" /></a></td>
            </tr>
        }
    </table>
</div>
<a asp-controller="User" asp-action="Create"><img src="~/Images/Icons/User.png" title="Pages" height="20" width="20" /> Create new user</a>
