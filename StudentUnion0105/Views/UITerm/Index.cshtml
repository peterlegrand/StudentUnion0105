@{
}@{
    ViewBag.Title = "Terms";
}
<h1 class="font-weight-bold">
    <img src="~/Images/Icons/Terms.png" height="60" width="60" /> Terms
</h1>
<div class="table-responsive">
    <table class="table table-bordered table-sm table-hover">

        <thead class="thead-dark">
            <tr><th>ID</th><th>Name</th><th></th><th></th><th></th><th></th><th></th><th></th></tr>
        </thead>
        @foreach (var x in Model)

        {
    <tr>
        <td style="width: 44px">@x.Id</td>
        <td>@x.Name</td>
        <td style="width: 22px"><a asp-controller="UITerm" asp-action="LanguageIndex" asp-route-id="@x.Id.ToString()"><img src="~/Images/Icons/languages.jpg" title="Languages" height="20" width="20" /></a></td>
    </tr>
        }
    </table>
</div>
