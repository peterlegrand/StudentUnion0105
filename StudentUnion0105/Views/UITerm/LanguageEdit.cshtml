@model SuUITermLanguageEditGetModel
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
@{
    ViewBag.Title = "Edit language";
}
<h1><img src="~/Images/Icons/languages.jpg" title="Languages" height="60" width="60" /> Edit language</h1>
<form class="mt-3" method="post">

    <div class="form-group row">
        <label asp-for="@Model.Id" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.Id" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.Customization" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.Customization" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.TermId" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.TermId" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.LanguageName" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.LanguageName" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.Name" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.Name" class="form-control" />
        </div>
    </div>
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary btn-dark">Update</button>
            <a asp-action="LanguageIndex" asp-controller="Page" class="btn btn-primary btn-light">Cancel</a>
        </div>
    </div>
</form>
