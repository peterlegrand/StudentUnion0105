@model List<StudentUnion0105.ViewModels.SuObjectVM>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}@{
    ViewBag.Title = "Steps for field";
}
<h1 class="font-weight-bold"><img src="~/Images/Icons/Step.jpg" title="Steps" height="60" width="60" /> Steps for field</h1>
<table  class="table table-bordered table-sm table-hover">

    <thead class="thead-dark">
        <tr><th>ID</th><th>Name</th><th></th><th></th><th></th><th></th></tr>
    </thead>
    @foreach (var x in Model)

    {
<tr>
    <td style="width: 44px">@x.Id</td>
    <td>@x.Description</td>
    <td>@x.Name</td>
    <td style="width: 22px"><a asp-controller="ProcessTemplateFieldSteps" asp-action="Edit" asp-route-Id="@x.Id.ToString()"><img src="~/Images/Icons/open.png" title="Edit" height="20" width="20" /></a></td>
</tr>
    }
</table>
<a asp-controller="ProcessTemplateField" asp-action="Index"><img src="~/Images/Icons/Field.png" title="Fields" height="20" width="20" /> Back to fields</a>

