@{
}@{
    ViewBag.Title = @ViewBag.Terms[0];
}
    <h1 class="font-weight-bold"><img src="~/Images/Icons/Step.jpg" title="@ViewBag.Terms[0]" height="60" width="60" /> @ViewBag.Terms[0]</h1>
<table  class="table table-bordered table-sm table-hover">

    <thead class="thead-dark">
        <tr><th>@ViewBag.Terms[1]</th><th>@ViewBag.Terms[2]</th><th></th><th></th><th></th><th></th></tr>
    </thead>
    @foreach (var x in Model)

    {
<tr>
    <td style="width: 44px">@x.Id</td>
    <td>@x.Description</td>
    <td>@x.Name</td>
    <td style="width: 22px"><a asp-controller="ProcessTemplateFieldSteps" asp-action="Edit" asp-route-Id="@x.Id.ToString()"><img src="~/Images/Icons/open.png" title="@ViewBag.Terms[3]" height="20" width="20" /></a></td>
</tr>
    }
</table>
<a asp-controller="ProcessTemplateField" asp-action="Index"><img src="~/Images/Icons/Field.png" title="@ViewBag.Terms[4]" height="20" width="20" /> @ViewBag.Terms[4]</a>

