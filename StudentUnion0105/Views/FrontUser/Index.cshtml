@model List<SuFrontUserIndexGetModel>
@{
    ViewBag.Title = ViewBag.Terms[0];
}
@{
    Layout = "_LayoutAdmin";
}
<h1 class="font-weight-bold">
    <img src="~/Images/Icons/Classifications.png" height="60" width="60" /> @ViewBag.Terms[0]
</h1>
<div class="table-responsive">
    <table class="table table-bordered table-sm table-hover">

        <thead>
            <tr><th>@ViewBag.Terms[1]</th><th>@ViewBag.Terms[2]</th><th>@ViewBag.Terms[3]</th><th></th><th></th><th></th><th></th></tr>
        </thead>
        @foreach (var x in Model)

        {
    <tr>
        <td>@x.UserId</td>
        <td>@x.UserName</td>
        <td>@x.Email</td>
        <td style="width: 22px"><a asp-controller="Classification" asp-action="Edit" asp-route-Id="@x.UserId"><img src="~/Images/Icons/open.png" title="@ViewBag.Terms[4]" height="20" width="20" /></a></td>
        <td style="width: 22px"><a asp-controller="Classification" asp-action="Edit" asp-route-Id="@x.UserId"><img src="~/Images/Icons/content.png" title="@ViewBag.Terms[5]" height="20" width="20" /></a></td>
        <td style="width: 22px"><a asp-controller="Classification" asp-action="Edit" asp-route-Id="@x.UserId"><img src="~/Images/Icons/Todo.png" title="@ViewBag.Terms[6]" height="20" width="20" /></a></td>
        <td style="width: 22px"><a asp-controller="Classification" asp-action="Edit" asp-route-Id="@x.UserId"><img src="~/Images/Icons/process.png" title="@ViewBag.Terms[7]" height="20" width="20" /></a></td>
    </tr>
        }
    </table>
</div>
