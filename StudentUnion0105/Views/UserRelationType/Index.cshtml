@model List<SuUserRelationTypeIndexGetModel>
@{
    ViewBag.Title = @ViewBag.Terms[0];
}@{
    Layout = "_LayoutAdmin";
}

<h1 class="font-weight-bold">
    <img src="~/Images/Icons/Project.png" height="60" width="60" /> @ViewBag.Terms[0]
</h1>
<div class="table-responsive">
    <table class="table table-bordered table-sm table-hover">

        <thead class="thead-dark">
            <tr><th>@ViewBag.Terms[1]</th><th>@ViewBag.Terms[2]</th><th>@ViewBag.Terms[7]</th><th>@ViewBag.Terms[8]</th><th>@ViewBag.Terms[9]</th></tr>
        </thead>
        @foreach (var x in Model)

        {
            <tr>
                <td>@x.OId</td>
                <td>@x.FromIsOfToName</td>
                <td>@x.ToIsOfFromName</td>
                <td>@x.FromIsOfToMenuName</td>
                <td>@x.ToIsOfFromMenuName</td>
                <td style="width: 22px"><a asp-controller="UserRelationType" asp-action="LanguageIndex" asp-route-id="@x.OId"><img src="~/Images/Icons/languages.jpg" title="<th>@ViewBag.Terms[10]</th>" height="20" width="20" /></a></td>
            </tr>
        }
    </table>
</div>
