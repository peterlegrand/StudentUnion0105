@model List<StudentUnion0105.ViewModels.SuObjectVM>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}@{
    ViewBag.Title = "Settings";
}@{
    Layout = "_LayoutAdmin";
}

<h1 class="font-weight-bold">
    <img src="~/Images/Icons/Settings.png" height="60" width="60" /> Settings
</h1>
<div class="table-responsive">
    <table class="table table-bordered table-sm table-hover col-6">

        <thead class="thead-dark">
            <tr><th>ID</th><th>Name</th><th>Int</th><th>String</th><th>Date</th><th></th><th></th><th></th></tr>
        </thead>
        @foreach (var x in Model)

        {
            <tr>
                <td style="width: 44px">@x.Id</td>
                <td>@x.Name</td>
                <td>@x.NullId</td>
                <td>@x.Description2</td>
                <td>@x.DateFrom</td>
                <td style="width: 22px"><a asp-controller="Setting" asp-action="Edit" asp-route-Id="@x.Id.ToString()"><img src="~/Images/Icons/open.png" title="Edit" height="20" width="20" /></a></td>
            </tr>
        }
    </table>
</div>
