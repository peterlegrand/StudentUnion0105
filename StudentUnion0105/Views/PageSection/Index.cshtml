@model List<SuObjectIndexGetModel>


@{
    ViewBag.Title = @ViewBag.Terms[0];
}
@{
    Layout = "_LayoutAdmin";
}
<h1 class="font-weight-bold"><img src="~/Images/Icons/Section.png" title="@ViewBag.Terms[0]" height="60" width="60" /> @ViewBag.Terms[0]</h1>
<table class="table table-bordered table-sm table-hover">

    <thead class="thead-dark">
        <tr><th>@ViewBag.Terms[1]</th><th>@ViewBag.Terms[2]</th><th></th><th></th><th></th><th></th></tr>
    </thead>
    @foreach (var x in Model)

    {
        <tr>
            <td style="width: 44px">@x.Id</td>
            <td>@x.Name</td>
            <td style="width: 22px"><a asp-controller="pageSection" asp-action="Edit" asp-route-Id="@x.Id.ToString()"><img src="~/Images/Icons/open.png" title="@ViewBag.Terms[3]" height="20" width="20" /></a></td>
            <td style="width: 22px"><a asp-controller="pageSectionPages" asp-action="Index" asp-route-id="@x.Id"><img src="~/Images/Icons/pages.jpg" title="Pages" height="20" width="20" /></a></td>
            <td style="width: 22px"><a asp-controller="pageSection" asp-action="LanguageIndex" asp-route-id="@x.Id.ToString()"><img src="~/Images/Icons/languages.jpg" title="Languages" height="20" width="20" /></a></td>
            <td style="width: 22px"><a asp-controller="pageSection" asp-action="Delete" asp-route-id="@x.Id.ToString()"><img src="~/Images/Icons/delete.png" title="@ViewBag.Terms[5]" height="20" width="20" /></a></td>
        </tr>
    }
</table>
<a asp-controller="PageSection" asp-action="Create" asp-route-id="@ViewBag.ObjectId">
    <img src="~/Images/Icons/Section.png" title="@ViewBag.Terms[6]" height="20" width="20" />
    @ViewBag.Terms[6]
</a>
<br />
<a asp-controller="Page" asp-action="Index">
    <img src="~/Images/Icons/pages.jpg" title="@ViewBag.Terms[7]" height="20" width="20" />
    @ViewBag.Terms[7]
</a>


