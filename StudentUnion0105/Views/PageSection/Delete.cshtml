        <div class="form-group row">
            <div class="col-sm-10">
                <button type="submit" class="btn btn-primary btn-dark">@ViewBag.Terms[10]</button>
                <a asp-action="Index" asp-controller="Organization" class="btn btn-primary btn-light">@ViewBag.Terms[11]</a>
            </div>
        </div>
