@model SuObjectVM
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}
@{
    ViewBag.Title = "Delete language";
}
<h1><img src="~/Images/Icons/languages.jpg" title="Languages" height="60" width="60" /> Edit language</h1>
<form class="mt-3" method="post">

    <div class="form-group row">
        <label asp-for="@Model.Id" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.Id" disabled class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.LanguageId" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.LanguageId" disabled class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.ObjectId" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.ObjectId"  class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.Name" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.Name" disabled class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.MouseOver" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.MouseOver" disabled class="form-control" />
            <span asp-validation-for="@Model.MouseOver" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.MenuName" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.MenuName" disabled class="form-control" />
            <span asp-validation-for="@Model.MenuName" class="text-danger"></span>
        </div>
    </div>
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary btn-dark">Delete</button>
            <a asp-action="LanguageIndex" asp-controller="ProcessTemplateFieldType" class="btn btn-primary btn-light">Cancel</a>
        </div>
    </div>
</form>

