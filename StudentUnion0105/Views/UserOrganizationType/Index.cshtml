@model List<SuObjectIndexGetModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}@{
    ViewBag.Title = @ViewBag.Terms[0];
}@{
    Layout = "_LayoutAdmin";
}

    <h1 class="font-weight-bold">
        <img src="~/Images/Icons/Organization.png" height="60" width="60" /> @ViewBag.Terms[0]
    </h1>
<div class="table-responsive">
    <table class="table table-bordered table-sm table-hover">

        <thead class="thead-dark">
            <tr><th>@ViewBag.Terms[1]</th><th>@ViewBag.Terms[2]</th><th>@ViewBag.Terms[3]</th><th>@ViewBag.Terms[5]</th><th></th></tr>
        </thead>
        @foreach (var x in Model)

        {
            <tr>
                <td>@x.Id</td>
                <td>@x.Name</td>
                <td>@x.Description</td>
                <td style="width: 22px"><a asp-controller="UserOrganizationType" asp-action="LanguageIndex" asp-route-id="@x.Id"><img src="~/Images/Icons/languages.jpg" title="Edit" height="20" width="20" /></a></td>
            </tr>
        }
    </table>
</div>
