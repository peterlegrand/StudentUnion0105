@model List<SuObjectIndexGetModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}@{
    ViewBag.Title = @ViewBag.Terms[0];
}
@{
    Layout = "_LayoutAdmin";
}
<h1 class="font-weight-bold"><img src="~/Images/Icons/levels.png" title="@ViewBag.Terms[0]" height="60" width="60" /> @ViewBag.Terms[0]</h1>
<table class="table table-bordered table-sm table-hover">

    <thead class="thead-dark">
        <tr><th>@ViewBag.Terms[1]</th><th>@ViewBag.Terms[2]</th><th>@ViewBag.Terms[10]</th><th>@ViewBag.Terms[11]</th><th></th><th></th><th></th><th></th></tr>
    </thead>
    @foreach (var x in Model)

    {
        <tr>
            <td style="width: 44px">@x.Id</td>
            <td>@x.Name</td>
            <td>@x.MenuName</td>
            <td>@x.MouseOver</td>
            <td style="width: 22px"><a asp-controller="ClassificationLevel" asp-action="Edit" asp-route-Id="@x.Id.ToString()"><img src="~/Images/Icons/open.png" title="@ViewBag.Terms[6]" height="20" width="20" /></a></td>
            <td style="width: 22px"><a asp-controller="ClassificationLevelPages" asp-action="Index" asp-route-id="@x.Id"><img src="~/Images/Icons/pages.jpg" title="@ViewBag.Terms[7]" height="20" width="20" /></a></td>
            <td style="width: 22px"><a asp-controller="ClassificationLevel" asp-action="LanguageIndex" asp-route-id="@x.Id.ToString()"><img src="~/Images/Icons/languages.jpg" title="@ViewBag.Terms[8]" height="20" width="20" /></a></td>
            <td style="width: 22px"><a asp-controller="ClassificationLevel" asp-action="Delete" asp-route-id="@x.Id.ToString()"><img src="~/Images/Icons/delete.png" title="@ViewBag.Terms[9]" height="20" width="20" /></a></td>
        </tr>
    }
</table>
<a asp-controller="ClassificationLevel" asp-action="Create" asp-route-id="@ViewBag.PId"><img src="~/Images/Icons/levels.png" title="@ViewBag.Terms[3]" height="20" width="20" /> @ViewBag.Terms[3]</a>
<br />
<a asp-controller="Classification" asp-action="Index"><img src="~/Images/Icons/Classifications.png" title="Classifications" height="20" width="20" /> @ViewBag.Terms[4]</a>

