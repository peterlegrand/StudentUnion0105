@model List<StudentUnion0105.ViewModels.SuObjectVM>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}@{
    ViewBag.Title = "Process template Steps";
}@{
    Layout = "_LayoutAdmin";
}

<h1 class="font-weight-bold"><img src="~/Images/Icons/Step.png" title="Levels" height="60" width="60" /> Process template Steps</h1>
<table class="table table-bordered table-sm table-hover">

    <thead class="thead-dark">
        <tr><th>ID</th><th>Name</th><th></th><th></th><th></th><th></th></tr>
    </thead>
    @foreach (var x in Model)

    {
        <tr>
            <td style="width: 44px">@x.Id</td>
            <td>@x.Name</td>
            <td style="width: 22px"><a asp-controller="ProcessTemplateStep" asp-action="Edit" asp-route-Id="@x.Id.ToString()"><img src="~/Images/Icons/open.png" title="Edit" height="20" width="20" /></a></td>
            <td style="width: 22px"><a asp-controller="ProcessTemplateStepFields" asp-action="Index" asp-route-id="@x.Id"><img src="~/Images/Icons/Field.png" title="Fields" height="20" width="20" /></a></td>
            <td style="width: 22px"><a asp-controller="ProcessTemplateStep" asp-action="LanguageIndex" asp-route-id="@x.Id.ToString()"><img src="~/Images/Icons/languages.jpg" title="Languages" height="20" width="20" /></a></td>
            <td style="width: 22px"><a asp-controller="ProcessTemplateStep" asp-action="Delete" asp-route-id="@x.Id.ToString()"><img src="~/Images/Icons/delete.png" title="Delete" height="20" width="20" /></a></td>
        </tr>
    }
</table>
<a asp-controller="ProcessTemplateStep" asp-action="Create" asp-route-id="@ViewBag.ObjectId"><img src="~/Images/Icons/levels.png" title="Step" height="20" width="20" /> Create new Process template Step</a>
<br />
<a asp-controller="ProcessTemplate" asp-action="Index"><img src="~/Images/Icons/process.png" title="Process template" height="20" width="20" /> Back to process template</a>

