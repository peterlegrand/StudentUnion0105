@model StudentUnion0105.ViewModels.SuObjectVM
@{
    ViewBag.Title = "Edit Page type";
}
<h1><img src="~/Images/Icons/pageType.jpg" height="60" width="60" /> Edit Page type</h1>
<form class="mt-3" method="post">

    <div class="form-group row">
        <label asp-for="@Model.Id" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.Id" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.ObjectLanguageId" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.ObjectLanguageId" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.LanguageId" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.LanguageId" class="form-control" />
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.Name" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.Name" class="form-control" />
            <span asp-validation-for="@Model.Name" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.Description" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.Description" class="form-control" />
            <span asp-validation-for="@Model.Description" class="text-danger"></span>
        </div>
    </div>
    <div class="form-group row">
        <label asp-for="@Model.MouseOver" class="col-sm-2 col-form-label"></label>
        <div class="col-sm-10">
            <input asp-for="@Model.MouseOver" class="form-control" />
            <span asp-validation-for="@Model.MouseOver" class="text-danger"></span>
        </div>
    </div>
    <div asp-validation-summary="All" class="text-danger"></div>
    <div class="form-group row">
        <div class="col-sm-10">
            <button type="submit" class="btn btn-primary btn-dark">Update</button>
            <a asp-action="Index" asp-controller="PageType" class="btn btn-primary btn-light">Cancel</a>
        </div>
    </div>
</form>
