@model List<SuObjectIndexGetModel>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}@{
    ViewBag.Title = "User Projects type";
}@{
    Layout = "_LayoutAdmin";
}

<h1 class="font-weight-bold">
    <img src="~/Images/Icons/Project.png" height="60" width="60" /> User Project type
</h1>
<div class="table-responsive">
    <table class="table table-bordered table-sm table-hover">

        <thead class="thead-dark">
            <tr><th>ID</th><th>Name</th><th></th><th></th></tr>
        </thead>
        @foreach (var x in Model)

        {
            <tr>
                <td>@x.Id</td>
                <td>@x.Name</td>
                <td style="width: 22px"><a asp-controller="UserProjectType" asp-action="LanguageIndex" asp-route-id="@x.Id"><img src="~/Images/Icons/languages.jpg" title="Edit" height="20" width="20" /></a></td>
            </tr>
        }
    </table>
</div>