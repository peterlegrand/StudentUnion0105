@model  IEnumerable<SuLanguageModel>
    @{
        ViewData["Title"] = "Language";
        Layout = "~/Views/Shared/_Layout.cshtml";
    }

    <h1>    <img src="~/Images/Icons/languages.jpg" height="60" width="60" /> Languages</h1>
    <table class="table table-bordered table-sm table-hover col-6">

        <thead class="thead-dark">
            <tr><th>ID</th><th>Name</th><th>Foreign name</th><th>ISO6391</th><th>ISO6392</th><th></th></tr>
        </thead>
        @foreach (var x in Model)

        {
    <tr>
        <td style="width: 44px">@x.Id</td>
        <td style="width: 144px">@x.LanguageName</td>
        <td style="width: 144px">@x.ForeignName</td>
        <td style="width: 44px">@x.ISO6391</td>
        <td style="width: 44px">@x.ISO6392</td>
        <td style="width: 44px">@x.Active</td>
        <td style="width: 22px"><a asp-controller="Language" asp-action="Edit" asp-route-Id="@x.Id.ToString()"><img src="~/Images/Icons/open.png" title="Edit" height="20" width="20" /></a></td>
    </tr>
        }

    </table>