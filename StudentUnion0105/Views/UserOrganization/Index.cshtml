@model List<SuObjectVM>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}@{
    ViewBag.Title = "User organizations";
}
<h1 class="font-weight-bold">
    <img src="~/Images/Icons/Organization.png" height="60" width="60" /> User organization
</h1>
<div class="table-responsive">
    <table class="table table-bordered table-sm table-hover">

        <thead class="thead-dark">
            <tr><th>ID</th><th>Name</th><th>Type</th><th></th><th></th><th></th><th></th><th></th></tr>
        </thead>
        @foreach (var x in Model)

        {
    <tr>
        <td>@x.Id</td>
        <td>@x.Name</td>
        <td>@x.Description</td>
        <td style="width: 22px"><a asp-controller="UserOrganization" asp-action="Delete" asp-route-id="@x.Id"><img src="~/Images/Icons/delete.png" title="Delete" height="20" width="20" /></a></td>
    </tr>
        }
    </table>
</div>
<a asp-controller="UserOrganization" asp-action="Create"asp-route-id="@ViewBag.ObjectId"><img src="~/Images/Icons/Organization.png" title="Organization" height="20" width="20" /> Add organization</a>
